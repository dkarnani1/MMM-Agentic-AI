outputs/*.png
outputs/*.pdf
outputs/*.svg
outputs/*.html
outputs/*.json

# Jupyter
.ipynb_checkpoints/
*.ipynb

# IDE / editor
.idea/
.vscode/
*.code-workspace
*.sublime-project
*.sublime-workspace

# OS
.DS_Store
.DS_Store?
Thumbs.db
ehthumbs.db
Desktop.ini

# Packaging / distribution
*.egg-info/
dist/
build/
*.whl

# Test / coverage
.pytest_cache/
.coverage
htmlcov/
coverage.xml

# Logs
*.log
